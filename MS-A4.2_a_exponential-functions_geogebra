// is3D=is 3D applet using 3D view, AV=Algebra View, SV=Spreadsheet View, CV=CAS View, EV2=Graphics View 2, CP=Construction Protocol, PC=Probability Calculator DA=Data Analysis, FI=Function Inspector, macro=Macros
var views = {'is3D': 0,'AV': 0,'SV': 0,'CV': 0,'EV2': 0,'CP': 0,'PC': 0,'DA': 0,'FI': 0,'macro': 0};
var applet_a = new GGBApplet(parameters_a, '5.0', views);
var applet_b = new GGBApplet(parameters_b, '5.0', views);
var applet_c = new GGBApplet(parameters_c, '5.0', views);
var applet_d = new GGBApplet(parameters_d, '5.0', views);
var applet_e = new GGBApplet(parameters_e, '5.0', views);
var applet_f = new GGBApplet(parameters_f, '5.0', views);
var applet_g = new GGBApplet(parameters_g, '5.0', views);
window.onload = function() {
  applet_a.inject('y=2^x');
  applet_b.inject('y=a^x');
  applet_c.inject('y=a^-x');
  applet_d.inject('y=a^x+y=a^-x');
  applet_e.inject('y=ka^x')
  applet_f.inject('y=3^x')
  applet_g.inject('y=3^-x')
};
applet.setPreviewImage('data:image/gif;base64,R0lGODlhAQABAAAAADs=','https://www.geogebra.org/images/GeoGebra_loading.png','https://www.geogebra.org/images/applet_play.png');
